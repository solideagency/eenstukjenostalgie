{
  "name": "meesterproef-adamnet",
  "version": "1.0.0",
  "description": "application that will tell the story of the Amsterdammer",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "browserify public/js/*.js -o public/dist/bundle.js",
    "dev": "watch 'npm run build'",
    "watch-js": "watchify public/js/*.js -o public/dist/bundle.js -dv",
    "watch": "npm run build | npm run watch-js",
    "postinstall": "netlify-lambda install",
    "netlifystart": "./node_modules/.bin/netlify-lambda serve app",
    "netlifybuild": "./node_modules/.bin/netlify-lambda build app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s44s/meesterproef-adamnet.git"
  },
  "author": "suustenvoorde",
  "license": "ISC",
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "body-parser": "^1.18.3",
    "browserify": "^16.2.2",
    "circle-to-polygon": "^1.0.2",
    "ejs": "^2.7.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.1.2",
    "serverless-http": "^2.3.2",
    "shortid": "^2.2.8",
    "uuid": "^3.2.1",
    "webpack-node-externals": "^1.7.2",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "nodemon": "^1.17.5",
    "watchify": "^3.11.1"
  }
}
